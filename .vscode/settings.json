{
    "python.languageServer": "Default",
    // "python.languageServer": "None",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook",
      "*.gif": "gifPlayer.player",
      "*.webp": "cweijan.officeViewer"
    },
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
      "editorIndentGuide.activeBackground1": "#FF9966",
      "editorIndentGuide.background1": "#4D4C60",
      "editorCursor.foreground": "#FFBBFF",
      "terminalCursor.foreground": "#FFBBFF"
    },
    "workbench.statusBar.visible": true,
    "window.menuBarVisibility": "classic",
    "zenMode.hideLineNumbers": false,
    "files.autoSave": "onFocusChange",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
      "*.html": "html"
    },
    "editor.fontFamily": "Inconsolata, 'Inconsolata SemiCondensed', Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "editor.fontSize": 24,
    "editor.lineHeight": 1.1,
    "editor.fontLigatures": false,
    "editor.suggestFontSize": 20,
    "editor.suggestLineHeight": 0,
    "editor.inlineSuggest.enabled": false,
    "editor.codeLensFontFamily": "Inconsolata, 'Inconsolata SemiCondensed', Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "editor.wordWrap": "off",
    "editor.detectIndentation": true,
    "editor.autoIndent": "full",
    "editor.mouseWheelZoom": true,
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.roundedSelection": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorWidth": 0,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace":"all",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "gutter",
    "editor.tabCompletion": "on",
    "editor.insertSpaces": true,
    "diffEditor.wordWrap": "off",
    "editor.wordWrapColumn": 8000,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 2000,
    "editor.rulers": [
      80,
      120,
      150
    ],
    // "terminal.integrated.shell.linux": "/bin/bash",
    "terminal.integrated.fontFamily": "'Inconsolata SemiCondensed', Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "terminal.integrated.fontSize": 24,
    "terminal.integrated.lineHeight": 1.1,
    "debug.console.fontFamily": "'Inconsolata SemiCondensed', Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "debug.console.fontSize": 22,
    "debug.console.lineHeight": 24,
    "debug.openDebug": "openOnSessionStart",
    "debug.allowBreakpointsEverywhere": true,
    "debug.showInStatusBar": "always",
    "debug.console.wordWrap": false,
    "markdown.preview.fontFamily": "'Inconsolata SemiCondensed', Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "markdown.preview.fontSize": 18,
    "markdown.preview.lineHeight": 1.2,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.compactFolders": true,
    "explorer.decorations.colors": true,
    "extensions.ignoreRecommendations": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "indenticator.color.dark": "#66FF66",
    "indenticator.inner.color.dark": "#FF0066",
    "indenticator.inner.width": 1.5,
    "indenticator.style": "inset",
    "indenticator.width": 0.5,
    "indenticator.showHover": true,
    "path-intellisense.mappings": {},
    "path-intellisense.showHiddenFiles": true,
    "path-intellisense.autoSlashAfterDirectory": false,
    "diffEditor.renderSideBySide": true,
    "emmet.triggerExpansionOnTab": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.showExpandedAbbreviation": "always",
    "emmet.showAbbreviationSuggestions": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapLineLength": 3000,
    "html.format.indentHandlebars": true,
    "[html]": {
      "editor.defaultFormatter": null,
      "editor.autoIndent": "none",
      "editor.detectIndentation": false,
      "editor.tabSize": 2,
      "editor.comments.insertSpace": true
    },
    "css.validate": false,
    "scss.validate": false,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "[javascript]": {
      "editor.autoIndent": "advanced",
      "editor.codeLens": true,
      "editor.wordWrap": "off",
      "editor.tabSize": 3
    },
    "jshint.options": {
      "asi": true
    },
    "[json]": {
      "editor.quickSuggestions": {
        "strings": true
      },
      "editor.suggest.insertMode": "replace",
      "editor.tabSize": 2,
      "editor.defaultFormatter": "HookyQR.beautify"
    },
    "git.autofetch": false,
    "git.suggestSmartCommit": true,
    "bracket-pair-colorizer-2.colors": [
      "#E5C100",
      "#E7657F",
      "DodgerBlue",
      "#FFFCD6",
      "#D600FF",
      "#0BFF01",
      "#A47C48",
      "Violet",
      "#9FD7EF",
      "#868990"
    ],
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "diffEditor.codeLens": true,
