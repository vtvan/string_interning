    "thunder-client.codeSnippetLanguage": "cs-httpclient",
    "thunder-client.htmlView": "Preview",
    "kite.showWelcomeNotificationOnStartup": false,
    "notebook.markup.fontSize": 20,
    "notebook.lineNumbers": "on",
    "notebook.showCellStatusBar": "visible",
    "notebook.undoRedoPerCell": true,
    "notebook.dragAndDropEnabled": true,
    "notebook.showFoldingControls": "always",
    "notebook.outline.showCodeCells": true,
    "notebook.output.textLineLimit": 2000,
    "jupyter.jupyterServerType": "local",
    "jupyter.showOutlineButtonInNotebookToolbar": true,
    "[jsonc]": {
      "editor.defaultFormatter": "HookyQR.beautify"
    },
    "jupyter.askForKernelRestart": false,
    "sonarlint.ls.javaHome": "/home/alex/.vscode/extensions/sonarsource.sonarlint_managed-jre/jre/jdk-11.0.14.1+1-jre",
    "workbench.colorTheme": "Simple Black Theme",
    "editor.unicodeHighlight.includeStrings": true,
    "editor.unicodeHighlight.ambiguousCharacters": true,
    "editor.unicodeHighlight.includeComments": true,
    "[python]": {
      "editor.formatOnType": true
    },
    "editor.lightbulb.enabled": "off",
    "search.showLineNumbers": true,
    // "thunder-client.saveToWorkspace": true,
    "CodeGPT.query.language": "English",
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "jupyter.interactiveWindow.cellMarker.markdownRegex": "^(#\\s*%%\\s*\\[markdown\\]|#\\s*\\<markdowncell\\>)",
    "jupyter.interactiveWindow.cellMarker.decorateCells": "currentCell",
    "notebook.output.fontSize": 26,
    "notebook.output.lineHeight": 28,
    "notebook.output.fontFamily": "'Inconsolata SemiCondensed', Inconsolata, Ubuntu Mono, Iosevka, Fantasque Sans Mono, mononoki, agave, Fira Code, ProggyVector, Office Code Pro, Menlo, Consolas, 'jf open 粉圓 1.1', Noto Sans TC",
    "git.openRepositoryInParentFolders": "never",
    "window.zoomLevel": 2,
    "notebook.stickyScroll.enabled": true,
    "terminal.integrated.stickyScroll.enabled": true,
    "files.exclude": {
      "**/__pycache__": true
    },
    "multiCommand.commands": [
      {
        "command": "multiCommand.selectAndCopyWord",
        "sequence": [
          "editor.action.addSelectionToNextFindMatch",
          "editor.action.clipboardCopyAction"
        ]
      }
    ],
    "git.enableSmartCommit": true,
    "workbench.editor.enablePreview": false,
    "interactiveWindow.executeWithShiftEnter": true
